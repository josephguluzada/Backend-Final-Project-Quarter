@model AboutViewModel
@{
    ViewData["Title"] = "Index";
}


        <!-- BREADCRUMB AREA START -->
        <div class="ltn__breadcrumb-area text-left bg-overlay-white-30 bg-image " data-bs-bg="../assets/img/bg/14.jpg">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="ltn__breadcrumb-inner">
                            <h1 class="page-title">About Us</h1>
                            <div class="ltn__breadcrumb-list">
                                <ul>
                                    <li><a href="index.html"><span class="ltn__secondary-color"><i class="fas fa-home"></i></span> Home</a></li>
                                    <li>About Us</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- BREADCRUMB AREA END -->
        <!-- ABOUT US AREA START -->
        <div class="ltn__about-us-area pt-120--- pb-90 mt--30">
            <div class="container">
                <div class="row">
                    <div class="col-lg-6 align-self-center">
                        <div class="about-us-img-wrap about-img-left">
                            <img src="~/assets/img/others/@Model.Settings.AboutImage" alt="About Us Image">
                            <div class="about-us-img-info about-us-img-info-2 about-us-img-info-3">

                                @*<div class="ltn__video-img ltn__animation-pulse1">
                                    <img src="~/assets/img/others/8.png" alt="video popup bg image">
                                    <a class="ltn__video-icon-2 ltn__video-icon-2-border---" href="https://www.youtube.com/embed/X7R-q9rsrtU?autoplay=1&showinfo=0" data-rel="lightcase:myCollection">
                                        <i class="fa fa-play"></i>
                                    </a>
                                </div>*@
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6 align-self-center">
                        <div class="about-us-info-wrap">
                            <div class="section-title-area ltn__section-title-2--- mb-20">
                                <h6 class="section-subtitle section-subtitle-2 ltn__secondary-color">About Us</h6>
                                <h1 class="section-title">
                                    @Model.Settings.AboutTitle<span>.</span>
                                </h1>
                                <p>
                                    @Model.Settings.AboutDesc
                                </p>
                            </div>
                            <ul class="ltn__list-item-half clearfix">
                                @foreach (var item in Model.Abouts)
                                {
                                    <li>
                                        <i class="@item.Icon"></i>
                                        @item.Title
                                    </li>
                                }
                            </ul>
                            <div class="ltn__callout bg-overlay-theme-05  mt-30">
                                <p>
                                    "@Model.Settings.AboutSubDesc"
                                </p>
                            </div>
                            <div class="btn-wrapper animated">
                                <a asp-action="index" asp-controller="service" class="theme-btn-1 btn btn-effect-1">@Model.Settings.AboutUrlText</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- ABOUT US AREA END -->
        <!-- FEATURE AREA START ( Feature - 6) -->
        <div class="ltn__feature-area section-bg-1 pt-120 pb-90 mb-120---">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="section-title-area ltn__section-title-2--- text-center">
                            <h6 class="section-subtitle section-subtitle-2 ltn__secondary-color">Our Services</h6>
                            <h1 class="section-title">Our Main Focus</h1>
                        </div>
                    </div>
                </div>
                <div class="row ltn__custom-gutter--- justify-content-center">
                    @foreach (var service in Model.Services)
                    {
                        <div class="col-lg-4 col-sm-6 col-12">
                            <div class="ltn__feature-item ltn__feature-item-6 text-center bg-white  box-shadow-1">
                                <div class="ltn__feature-icon">
                                     <span><i class="@service.Icon"></i></span> 
                                    @*<img src="~/assets/img/icons/icon-img/21.png" alt="#">*@
                                </div>
                                <div class="ltn__feature-info">
                                    <h3><a href="service-details.html">@service.Title</a></h3>
                                    <p>@service.Desc</p>
                                    <a class="ltn__service-btn" href="service-details.html">@service.UrlText <i class="flaticon-right-arrow"></i></a>
                                </div>
                            </div>
                        </div>

                    }
                </div>
            </div>
        </div>
        <!-- FEATURE AREA END -->
        <!-- TEAM AREA START (Team - 3) -->
        <div class="ltn__team-area pt-115 pb-90">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="section-title-area ltn__section-title-2--- text-center">
                            <h6 class="section-subtitle section-subtitle-2 ltn__secondary-color">Team</h6>
                            <h1 class="section-title">Sale Managers</h1>
                        </div>
                    </div>
                </div>
                <div class="row justify-content-center">
                    @foreach (var item in Model.SaleManagers)
                    {
                        <div class="col-lg-4 col-sm-6">
                            <div class="ltn__team-item ltn__team-item-3---">
                                <div class="team-img">
                                    <img src="~/assets/img/team/@item.Image" alt="Image">
                                </div>
                                <div class="team-info">
                                    <h4><a href="team-details.html">@item.FullName</a></h4>
                                    <h6 class="ltn__secondary-color">Sale Manager</h6>
                                    <div class="ltn__social-media">
                                        <ul>
                                            <li><a href="@item.FacebookUrl"><i class="fab fa-facebook-f"></i></a></li>
                                            <li><a href="@item.TwitterUrl"><i class="fab fa-twitter"></i></a></li>
                                            <li><a href="@item.LinkedInUrl"><i class="fab fa-linkedin"></i></a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }

                </div>
            </div>
        </div>
        <!-- TEAM AREA END -->
